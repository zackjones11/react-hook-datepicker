{
  "name": "@zackjones11/react-hook-datepicker",
  "version": "1.0.0",
  "description": "A set of React Hooks to help build both single and range datepickers",
  "main": "index.js",
  "scripts": {
    "test:unit": "jest /src",
    "test:unit:watch": "npm run test:unit -- --watchAll",
    "lint": "eslint *.{ts,tsx}",
    "lint:fix": "yarn lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zackjones11/react-hook-datepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "rangedatepicker",
    "calendar"
  ],
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "react": "*",
    "typescript": "^4.2.3"
  },
  "author": "Zack Jones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zackjones11/react-hook-datepicker/issues"
  },
  "homepage": "https://github.com/zackjones11/react-hook-datepicker#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:types && lint-staged"
    }
  },
  "lint-staged": {
    "*.{json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
