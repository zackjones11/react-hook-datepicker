{
  "name": "react-hook-datepicker",
  "version": "1.3.0",
  "description": "A set of React Hooks to help build both single and range datepickers",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "test:unit": "jest /src",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zackjones11/react-hook-datepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "range",
    "date input",
    "calendar",
    "useDatePicker",
    "useDateRangePicker"
  ],
  "peerDependencies": {
    "date-fns": "^2.19.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "date-fns": "*",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "react": "*",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "author": "Zack Jones (https://zackjones.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackjones11/react-hook-datepicker/issues"
  },
  "homepage": "https://github.com/zackjones11/react-hook-datepicker#readme",
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
