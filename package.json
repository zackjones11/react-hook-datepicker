{
  "name": "react-hook-datepicker",
  "version": "1.3.0",
  "description": "A set of React Hooks to help build both single and range datepickers",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "test:unit": "jest /src",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zackjones11/react-hook-datepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "range",
    "date input",
    "calendar",
    "useDatePicker",
    "useDateRangePicker"
  ],
  "peerDependencies": {
    "date-fns": ">=2.0.0",
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@jest/types": "29.3.1",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.5",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "date-fns": "2.29.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "13.1.0",
    "react": "18.2.0",
    "rollup": "3.9.0",
    "rollup-plugin-typescript2": "0.34.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "author": "Zack Jones (https://zackjones.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackjones11/react-hook-datepicker/issues"
  },
  "homepage": "https://github.com/zackjones11/react-hook-datepicker#readme",
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
